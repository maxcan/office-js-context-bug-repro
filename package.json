{
    "name": "word-reformatter",
    "description": "",
    "author": "",
    "version": "1.0.0",
    "scripts": {
        "clean": "rimraf dist && rimraf .awcache",
        "lint": "tslint --project tsconfig.json",
        "start": "yarn run gen-git && webpack-dev-server --inline --hot --config config/webpack.dev.js --progress",
        "deploy": " aws s3 sync dist/ s3://word-formatter.tools.atrium.co/  ",
        "build": "yarn run clean && yarn run gen-git && webpack --config config/webpack.prod.js --colors --progress --bail",
        "install-manifest": "mkdir -p ~/Library/Containers/com.microsoft.Word/Data/Documents/wef && cp manifests/dev/localshim.xml ~/Library/Containers/com.microsoft.Word/Data/Documents/wef",
        "gen-git": "echo \"export default \\`$(if [[ -n $(git status --porcelain) ]]; then echo dirty-; fi)$(git rev-parse HEAD)\\`\" > src/norepo-git.ts",
        "gen-ssl": "cd certs && openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout server.key -out localshim.crt ",
        "validate": "./node_modules/.bin/validate-office-addin"
    },
    "dependencies": {
        "@microsoft/office-js-helpers": "^0.7.4",
        "bluebird": "^3.5.1",
        "bootstrap": "^4.0.0",
        "cheerio": "^1.0.0-rc.2",
        "core-js": "^2.4.1",
        "office-addin-validator": "^1.0.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-icons": "^2.2.7",
        "react-transition-group": "^2.2.1",
        "reactstrap": "^4.8.0",
        "uuid": "^3.2.1"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.20",
        "@types/bootstrap": "^3.3.37",
        "@types/cheerio": "^0.22.7",
        "@types/office-js": "^0.0.51",
        "@types/react": "16.0.36",
        "@types/react-dom": "16.0.3",
        "@types/react-icons": "^2.2.4",
        "@types/reactstrap": "^5.0.13",
        "@types/uuid": "^3.4.3",
        "@types/webpack": "2.2.12",
        "@types/webpack-dev-server": "1.12.6",
        "autoprefixer": "6.7.7",
        "awesome-typescript-loader": "3.1.2",
        "browser-sync": "2.18.8",
        "browser-sync-webpack-plugin": "1.1.4",
        "copy-webpack-plugin": "4.0.1",
        "css-loader": "0.27.3",
        "extract-text-webpack-plugin": "2.1.0",
        "file-loader": "0.10.1",
        "html-webpack-plugin": "2.28.0",
        "json": "^9.0.6",
        "node-sass": "4.5.1",
        "perfectionist": "2.4.0",
        "postcss-loader": "1.3.3",
        "precss": "1.4.0",
        "rimraf": "2.6.1",
        "sass-loader": "6.0.3",
        "style-loader": "0.16.0",
        "ts-loader": "^3.4.0",
        "tslint": "4.5.1",
        "typescript": "^2.7.1",
        "webpack": "2.3.1",
        "webpack-dev-server": "2.4.2",
        "webpack-merge": "4.1.0"
    }
}
